<% provide :title, t("accounts.sign_up") %>

<div class="row">
  <div class="col-md-6 offset-md-3">
    <h1 class = "sign-up-title"><%= uppercase t("accounts.sign_up") %></h1>
    <%= form_for resource, as: resource_name, url: registration_path(resource_name) do |f| %>
      <%= render "form", f: f %>
    <% end %>
  </div>
</div>
